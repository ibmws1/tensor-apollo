{
  "manifest_version": 3,
  "name": "Douyin Compass Pro",
  "version": "1.1",
  "description": "Auto-load 200 items, Price Filters, Video Enhancements (Chrome Optimized)",
  "host_permissions": [
    "*://compass.jinritemai.com/*"
  ],
  "permissions": [
    "webRequest"
  ],
  "content_scripts": [
    {
      "matches": [
        "*://compass.jinritemai.com/*"
      ],
      "js": [
        "injector.js"
      ],
      "run_at": "document_start",
      "world": "ISOLATED"
    }
  ],
  "web_accessible_resources": [
    {
      "resources": [
        "loader.js",
        "globals.js",
        "utils.js",
        "interceptor.js",
        "render.js",
        "navigation.js",
        "download.js",
        "ui_filter.js",
        "video_enhancer.js",
        "actions.js",
        "ui.js"
      ],
      "matches": [
        "*://compass.jinritemai.com/*"
      ]
    }
  ]
}